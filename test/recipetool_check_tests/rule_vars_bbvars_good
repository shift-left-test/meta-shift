PREMIRRORS ?= "foo"
PRSERV_HOST ?= "foo"
STAMP ?= "foo"
TOPDIR ?= "foo"
BB_CONSOLELOG ??= "foo"
BB_CURRENTTASK ??= "foo"
BB_DANGLINGAPPENDS_WARNONLY ??= "foo"
