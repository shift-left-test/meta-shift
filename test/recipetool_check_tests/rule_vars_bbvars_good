#!/bin/sh
TMP="$1/"
TOOL="recipetool check"
TESTFILE="${TMP}oelint_adv_test.bb"
trap "rm -f ${TESTFILE}" EXIT
TESTID="oelint.vars.bbvars.*"
TESTOCC="0"
cat > ${TESTFILE} << EOF
BB_CONSOLELOG ?= "foo"
BB_CURRENTTASK ?= "foo"
BB_DANGLINGAPPENDS_WARNONLY ?= "foo"
