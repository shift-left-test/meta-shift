BB_WORKERCONTEXT ?= "foo"
BBDEBUG ?= "foo"
BBFILE_COLLECTIONS ?= "foo"
BBFILE_PATTERN ?= "foo"
BBFILE_PRIORITY ?= "foo"
BBFILES ?= "foo"
